# whalefox.env

################## CHANGE THIS AND RENAME IT ##################

### Gluetun VPN configuration
VPN_SERVICE_PROVIDER=
VPN_TYPE=
VPN_PORT_FORWARDING=
VPN_PORT_FORWARDING_PROVIDER=
FIREWALL_OUTBOUND_SUBNETS=
HTTPPROXY=
BLOCK_MALICIOUS=on
BLOCK_SURVEILLANCE=on
BLOCK_ADS=on

### Port to open for Firefox container
FF_PORT=3000

# Only required for OpenVPN
OPENVPN_USER=
OPENVPN_PASSWORD=

# Only required for Wireguard
WIREGUARD_PRIVATE_KEY=
SERVER_COUNTRIES=
WIREGUARD_ADDRESSES=
WIREGUARD_PUBLIC_KEY=
WIREGUARD_ENDPOINT_IP=
WIREGUARD_ENDPOINT_PORT=

# Firefox browser configuration
PUID=1000
PGID=1000
LC_ALL=
TZ=Etc/UTC
HTTP_PROXY=
HTTPS_PROXY=
SHM_SIZE=2g
FIREFOX_CLI=